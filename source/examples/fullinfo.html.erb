---
title: Fullpage Infographic Layout
layout: blank
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title><%= current_page.data.title || "Wholepage" %></title>

    <%= stylesheet_link_tag "app" %>
    <%= stylesheet_link_tag "custom" %>
</head>

<body class="whole-page infographic-page">
  <span id="page-top"></span>
  <div class="container infographic-container">
    <main role="main">>
      <h1>Title here</h1>
      <p>Some content here.</p>
    </main>
    <footer id="footer">
      <div class="row">
        <div class="col-lg-12">

          <ul class="list-unstyled">
            <li class="float-lg-right"><a href="#page-top">Back to top</a></li>
            <li>Copyright &copy; 2020 King's College London</li>
          </ul>

        </div>
      </div>
    </footer>
  </div>

  <%= javascript_include_tag "app" %>
</body>

</html>
